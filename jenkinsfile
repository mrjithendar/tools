pipeline {
    agent any

    options {
        disableConcurrentBuilds()
    }

    stages {
        stage('VAULT TEST') {
            steps {
                withVault(configuration: [skipSslVerification: true, timeout: 60, vaultCredentialId: 'vault-jenkins', vaultUrl: 'test'], vaultSecrets: [[path: 'secrets/creds/git-pass', secretValues: [[envVar: 'password', vaultKey: 'test-git-creds']]]]) {
                    sh "echo $envVar"
                }
            }
        }
    } 
}