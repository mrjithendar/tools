pipeline {
    agent any

    options {
        disableConcurrentBuilds()
    }

    stages {
        stage('VAULT TEST') {
            steps {
                withVault(configuration: [skipSslVerification: true, timeout: 60, vaultCredentialId: 'vault-jenkins', vaultUrl: 'http://127.0.0.1:8200'], vaultSecrets: [[path: 'secrets/creds/my-secret-text', secretValues: [[envVar: 'password', vaultKey: 'secret']]]]) {
                    sh "echo ${env.password}"
                }
            }
        }
    } 
}